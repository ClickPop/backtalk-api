{
  "name": "survey-app-server",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "scripts": {
    "setup": "npm install && sequelize-cli db:create",
    "migrate": "sequelize-cli db:migrate",
    "migrate:fresh": "sequelize-cli db:migrate:undo:all && sequelize-cli db:migrate",
    "seed": "sequelize-cli db:seed:all",
    "seed:fresh": "sequelize-cli db:seed:undo:all && npm run seed",
    "start": "nodemon app.js",
    "lint": "eslint --cache --fix",
    "prettier": "prettier --write *.{js,json,md,scss} **/*.{js,json,md,scss} !node_modules/**.*"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.8.1",
    "eslint-config-prettier": "^6.11.0",
    "husky": "^4.3.0",
    "lint-staged": "^10.3.0",
    "lorem-ipsum": "^2.0.3",
    "nodemon": "^2.0.4",
    "prettier": "^2.1.1",
    "sequelize-cli": "^6.2.0"
  },
  "dependencies": {
    "bcrypt": "^5.0.0",
    "express": "^4.17.1",
    "morgan": "^1.10.0",
    "pg": "^8.3.3",
    "sequelize": "^6.3.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "*.{js,md,json}": "prettier --write"
  }
}
